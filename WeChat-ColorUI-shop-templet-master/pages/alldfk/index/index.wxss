page {
  padding-bottom: 20rpx;
  padding-top: 50px;
}
.head-title {
  margin: 0 auto;
}
.cu-custom .cu-bar {
  background-color: #fff;
  padding-right: inherit;
}
.line-green,
.lines-green,
.text-green {
  color: #e45c5c;
}
.swiperitem {
  background: #fff;
  height: 55px;
}
.canui-video {
  height: 425rpx;
  width: 100%;
}
.nav-li,
.nav-list {
  padding: 0;
  z-index: 0;
}
.canui-yx-url {
  margin: 0 2% 30rpx !important;
  width: 46% !important;
}
.canui-flex-car {
  padding-top: 10px;
}
.canui-flex-car .cu-card > .cu-item {
  margin: 0;
  position: relative;
}
.canui-flex-car .cu-list.menu-avatar > .cu-item {
  height: 180rpx;
  padding-left: 15rpx;
}
.canui-flex-car .cu-card .cu-avatar {
  bottom: 72rpx;
  position: absolute;
  right: 10px;
}
.canui-flex-car .shadow-warp {
  box-shadow: 0 0 30rpx rgba(0, 0, 0, 0.1);
}
.canui-flex-car .cu-list.menu > .cu-item .content {
  font-size: 26rpx;
}
.canui-flex-car .text-sm {
  font-size: 20rpx;
}
.canui-flex-car .text-grey {
  color: #000;
}
.canui-flex-car image {
  height: 173px !important;
}
.canui-gps .justify-between {
  padding: 0 10px;
}
.content .text-sm {
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 100px;
}
button::after {
  border: none;
}
.btns {
  height: 180rpx;
}
.submitbtn {
  background-color: #c3a769;
  border: none;
  border-radius: 35px;
  color: #fff;
  float: right;
  font-size: 15px;
  font-weight: 700;
}
.tp {
  margin: 10px 5px 5px;
}
.canui-fixed {
  bottom: 0;
  position: fixed;
  width: 100%;
  z-index: 999999;
}
.tt {
  margin: 0 0 50px;
}
.pt {
  margin: 0 0 15px px;
}
.Grid-sku {
  display: -webkit-flex;
  display: flex;
  width: 100%;
}
checkbox .wx-checkbox-input {
  border-radius: 50%;
  height: 35rpx;
  line-height: 35rpx;
  width: 35rpx;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  background-color: #c3a769 !important;
  border: 1rpx solid #c3a769 !important;
  height: 35rpx;
  line-height: 35rpx;
  width: 35rpx;
}
.btns {
  background-color: #fff;
  bottom: 0;
  display: -webkit-flex;
  display: flex;
  height: 80rpx;
  justify-content: space-between;
  position: fixed;
  width: 100%;
  z-index: 1;
}
.resetbtn {
  background-color: #c3a769;
  color: #fff;
  font-size: 12px;
  font-weight: 700;
  margin-bottom: 30px;
  width: 50% !important;
}
.text_qipao {
  color: #fff;
  font-size: 25rpx;
}
.radio {
  padding: 5px;
}
.is_checked {
  color: #c3a769;
  font-weight: 700;
}
.normal {
  color: #000;
}
