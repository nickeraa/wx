.UCenter-bg {
  align-items: center;
  background-image: url(https://widesky.work/HKback/upload/bg2.jpg);
  background-size: cover;
  color: #fff;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  font-weight: 300;
  height: 550rpx;
  justify-content: center;
  overflow: hidden;
  padding-top: 40rpx;
  position: relative;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}
.UCenter-bg text {
  opacity: 0.8;
}
