.screen-swiper {
  min-height: 555rpx;
}
.canui-head-box {
  background: rgba(0, 0, 0, 0.2);
  border: 1px solid hsla(0, 0%, 100%, 0.3);
  border-radius: 20px;
  color: #fff;
  margin-left: 10px;
  padding: 4px;
  position: fixed;
  top: 26px;
}
.canui-head-box navigator {
  display: initial;
  font-size: 20px;
  padding: 8px;
}
.canui-head-box navigator + navigator {
  border-left: 1px solid hsla(0, 0%, 100%, 0.3);
  margin: 0 3px;
  padding: 0 5px 0 15px;
}
.canui-goods-box {
  padding: 10rpx 30rpx;
}
.canui-xzwz {
  -webkit-line-clamp: 2;
}
.cauni-details-box {
  word-wrap: break-word;
}
.cu-bar.tabbar.shop .action {
  width: 100rpx;
}
.cu-bar.tabbar .action {
  font-size: 20rpx;
}
.cu-bar.tabbar .action [class*="icon"] {
  width: 38rpx;
}
.gp {
  margin-left: 20px;
}
.bgpp {
  color: red;
  font-size: 26rpx;
}
.bgcolor {
  height: 100%;
}
.bgcolor,
.cp {
  background: #fff;
}
.cp {
  height: 88px;
}
.up {
  text-transform: Uppercase;
}
.previewimg {
  height: 100%;
  width: 100%;
}
.input {
  border: 1px gray;
  padding: 10rpx;
}
